{
	"AWSTemplateFormatVersion":"2010-09-09",
	"Description":"test template for cnfassist - creates a single subnet, ZoneA required as a parameter",
	"Parameters":{
		"env":{
			"Type":"String"
		},
		"vpc":{
			"Type":"String"
		},
		"zoneA":{
			"Type":"String",
			"Description":"zoneADescription"
		},
		"zoneB":{
			"Type":"String",
			"Default":"eu-west-1b"
		}
		
	},
	"Resources":{
		"testSubnet":{
			"Type":"AWS::EC2::Subnet",
			"Properties":{
				"AvailabilityZone":{
					"Ref":"zoneA"
				},
				"CidrBlock":"10.0.10.0/24",
				"Tags":[
					{
						"Key":"Name",
						"Value":"webSubnetZoneA"
					},
					{
						"Key":"TagEnv",
						"Value":{
							"Ref":"env"
						}
						
					}
				],
				"VpcId":{
					"Ref":"vpc"
				}
				
			}
			
		}
		
	}
}